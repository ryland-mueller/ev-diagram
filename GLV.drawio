        <mxCell id="v1pvGCh-GkigHLZ5UNmK-15" value="&quot;Throttle Demand&quot; and &quot;Regen Demand&quot; signals are sent over DIgital Pedal Bus (DPB) which will be a fault-tolerant CAN bus" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontColor=#000000;fontSize=5;fontStyle=0" parent="1" vertex="1">
          <mxGeometry x="800" y="130" width="220" height="20" as="geometry" />
        </mxCell>
        <mxCell id="v1pvGCh-GkigHLZ5UNmK-16" value="&lt;b&gt;ALL&lt;/b&gt; BSE and APPS outputs must facilitate detection of wiring failure, ie Normally Closed contact, 0.5-4.5V, 4-20mA, etc (PENDING RULES CLARIFICATION)" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=bottom;whiteSpace=wrap;rounded=0;fontColor=#000000;fontSize=5;fontStyle=0" parent="1" vertex="1">
          <mxGeometry x="430" y="10" width="149" height="19.43" as="geometry" />
        </mxCell>
        <mxCell id="v1pvGCh-GkigHLZ5UNmK-17" value="BSPD must be completely non-programmable, so inputs must be direct (ie not over any digital bus). Buffered outputs of inputs will be provided for other devices.&lt;br&gt;&lt;br&gt;Since BSPD takes analog inputs from both front and rear of car, it will be positioned in the middle of the car longitudinally" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=top;whiteSpace=wrap;rounded=0;fontColor=#000000;fontSize=5;fontStyle=0;spacingTop=0;" parent="1" vertex="1">
          <mxGeometry x="570" y="440" width="130" height="60" as="geometry" />
        </mxCell>
        <mxCell id="v1pvGCh-GkigHLZ5UNmK-18" value="Values of all inputs and outputs are additionally sent over regular CAN bus for logging/monitoring/troubleshooting" style="text;html=1;strokeColor=none;fillColor=none;align=center;verticalAlign=middle;whiteSpace=wrap;rounded=0;fontColor=#000000;fontSize=5;fontStyle=0" parent="1" vertex="1">
          <mxGeometry x="800" y="200" width="130" height="20" as="geometry" />
        </mxCell>
